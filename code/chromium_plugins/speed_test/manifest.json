{
  "manifest_version": 3,
  "name": "Real Speed Test - GitHub Video",
  "version": "1.0",
  "description": "Measures download speed by fetching an 8K video file from GitHub.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
